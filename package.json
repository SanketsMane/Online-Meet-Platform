{
    "name": "kidokoolsfu",
    "version": "2.1.06",
    "description": "WebRTC SFU browser-based video calls",
    "main": "Server.js",
    "scripts": {
        "test": "mocha tests/*.js",
        "start": "node app/src/Server.js",
        "start-dev": "nodemon app/src/Server.js",
        "debug": "DEBUG='mediasoup*' node app/src/Server.js",
        "compile": "npx webpack",
        "lint": "npx prettier --write .",
        "docker-build": "docker build --tag kidokool/sfu:latest .",
        "docker-rmi": "docker image prune -f",
        "docker-push": "docker push kidokool/sfu:latest",
        "docker-pull": "docker pull kidokool/sfu:latest",
        "docker-rmc": "docker container rm kidokoolsfu --force",
        "docker-run": "docker run -d -p 40000-40100:40000-40100 -p 3010:3010 -v ./.env:/src/.env -v ./app/src/config.js:/src/app/src/config.js:ro --name kidokoolsfu kidokool/sfu:latest",
        "docker-run-vm": "docker run -d -p 40000-40100:40000-40100 -p 3010:3010 -v ./.env:/src/.env -v ./app/:/src/app/:ro -v ./public/:/src/public/:ro --name kidokoolsfu kidokool/sfu:latest",
        "docker-start": "docker start kidokoolsfu",
        "docker-stop": "docker stop kidokoolsfu",
        "rtmp-start": "docker-compose -f rtmpServers/nginx-rtmp/docker-compose.yml up -d",
        "rtmp-stop": "docker-compose -f rtmpServers/nginx-rtmp/docker-compose.yml down",
        "rtmp-restart": "docker-compose -f rtmpServers/nginx-rtmp/docker-compose.yml down && docker-compose -f rtmpServers/nginx-rtmp/docker-compose.yml up -d",
        "rtmp-logs": "docker logs -f kidokool-rtmp",
        "nms-node-start": "node rtmpServers/node-media-server/src/server.js",
        "nms-start": "docker-compose -f rtmpServers/node-media-server/docker-compose.yml up -d",
        "nms-stop": "docker-compose -f rtmpServers/node-media-server/docker-compose.yml down",
        "nms-restart": "docker-compose -f rtmpServers/node-media-server/docker-compose.yml down && docker-compose -f rtmpServers/node-media-server/docker-compose.yml up -d",
        "nms-logs": "docker logs -f kidokool-nms"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SanketsMane/Online-Meet-Platform.git"
    },
    "keywords": [
        "webrtc",
        "socket.io",
        "broadcasting",
        "sfu",
        "nodejs",
        "video",
        "audio",
        "openai",
        "oidc",
        "chatgpt",
        "rtmp",
        "poll",
        "client",
        "server",
        "streaming",
        "realtime"
    ],
    "author": "Miroslav Pejic",
    "license": "AGPL-3.0",
    "engines": {
        "node": ">=22"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.984.0",
        "@aws-sdk/lib-storage": "^3.984.0",
        "@mattermost/client": "11.3.0",
        "@ngrok/ngrok": "1.7.0",
        "@sentry/node": "^10.38.0",
        "async-mutex": "^0.5.0",
        "axios": "^1.13.4",
        "bcrypt": "^6.0.0",
        "chokidar": "^5.0.0",
        "colors": "1.4.0",
        "compression": "1.8.1",
        "cors": "2.8.6",
        "crypto-js": "4.2.0",
        "discord.js": "^14.25.1",
        "dompurify": "^3.3.1",
        "dotenv": "^17.2.4",
        "express": "5.2.1",
        "express-openid-connect": "^2.19.4",
        "express-rate-limit": "^8.2.1",
        "fluent-ffmpeg": "^2.1.3",
        "he": "^1.2.0",
        "helmet": "^8.1.0",
        "httpolyglot": "0.1.2",
        "js-yaml": "^4.1.1",
        "jsdom": "^28.0.0",
        "jsonwebtoken": "^9.0.3",
        "mediasoup": "3.19.17",
        "mediasoup-client": "3.18.6",
        "mime-types": "^3.0.2",
        "nodemailer": "^8.0.0",
        "openai": "^6.18.0",
        "qs": "6.14.1",
        "sanitize-filename": "^1.6.3",
        "sequelize": "^6.37.7",
        "socket.io": "4.8.3",
        "sqlite3": "^5.1.7",
        "swagger-ui-express": "5.0.1",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.29.0",
        "@babel/preset-env": "^7.29.0",
        "babel-loader": "^10.0.0",
        "mocha": "^11.3.0",
        "node-fetch": "^3.3.2",
        "nodemon": "^3.1.11",
        "prettier": "3.8.1",
        "proxyquire": "^2.1.3",
        "should": "^13.2.3",
        "sinon": "^21.0.1",
        "webpack": "^5.105.0",
        "webpack-cli": "^6.0.1"
    }
}
